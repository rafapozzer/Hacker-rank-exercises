#include <stdio.h> 
#include <iostream> 
using namespace std; 

 
void update(int *a,int *b) { 
int sum, dif; 
if (*a>=*b){ 
dif = *a - *b; 
} 

else{ 
dif = *b - *a; 
}

sum = *a + *b; 
*a = sum; 
*b = dif; 
} 

int main() { 
int a, b; 
int *pa = &a, *pb = &b; 
cin>>a>>b; 
update(pa, pb); 
cout<<a<<endl<<b<<endl;

return 0; 
} 
