#include <iostream> 
#include <string> 
using namespace std; 

int main(){
    string a,b;
    cin>>a>>b;
    char c = a[0];
    char d = b[0];
    char e,f;
    int tam = a.size();
    int tam_1 = b.size();
    
    cout<<tam<<" "<<tam_1<<endl;
    cout<<a<<b<<endl;
    cout<<d;
   
    for(int i=1; i <= tam; i++){
        e = a[i];
        cout<<e;
        }
        
    cout<<" "<<c;
    for(int j=1; j<=tam_1; j++){
        f = b[j];
        cout<<f;
    }
    
    return 0;   
}
