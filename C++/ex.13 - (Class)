#include <iostream> 
#include <sstream> 
using namespace std; 
 

class Student{ 

private: 
int age, standard; 
string first_name, last_name, to__string; 

public: 

void set_age(int a){ 
age = a; 
} 

int get_age(){ 
return age; 
} 

public: 

void set_first_name(string str){ 
first_name = str; 
} 

string get_first_name(){ 
return first_name; 
} 

public: 

void set_last_name(string str1){ 
last_name = str1; 
} 

string get_last_name(){ 
return last_name; 
} 

public: 

void set_standard(int a1){ 
standard = a1; 
} 

int get_standard(){ 
return standard; 
} 

public: 
string to_string(){ 
stringstream ss; 
string to__string; 
ss<<age<<","; 
ss<<first_name<<","; 
ss<<last_name<<","; 
ss<<standard; 
to__string = ss.str(); 

return to__string; 
} 
};  

int main() { 

int age, standard; 
string first_name, last_name, to__string; 
cin >> age >> first_name >> last_name >> standard; 

Student st; 
st.set_age(age); 
st.set_standard(standard); 
st.set_first_name(first_name); 
st.set_last_name(last_name); 

cout << st.get_age() << "\n"; 
cout << st.get_last_name() << ", " << st.get_first_name() << "\n"; 
cout << st.get_standard() << "\n"; 
cout << "\n"; 
cout << st.to_string(); 

return 0; 
} 
